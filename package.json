{
	"name": "fairgnuegen",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "npm run build:content && react-router dev",
		"start": "react-router-serve ./build/server/index.js",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint \"app/**/*\"",
		"prettier": "prettier --write .",
		"prettier:ci": "prettier --check .",
		"build:update-data-and-translate": "tsx --tsconfig ./tsconfig.app.json data/update-data-and-translate.ts && prettier --config ./prettier.config.js --write data/fairgnuegen_data.json",
		"build:content": "tsx --tsconfig ./tsconfig.app.json data/generate-content-and-routes.ts && prettier --config prettier.config.js --write app/content/detail-pages-content.ts react-router.config.ts",
		"build": "NODE_ENV=production npm run build:content && react-router build",
		"preview": "serve build/client",
		"test:visual": "playwright test --config=tests/visual/playwright.config.ts"
	},
	"dependencies": {
		"@react-router/node": "7.1.5",
		"exponential-backoff": "3.1.2",
		"isbot": "5.1.33",
		"maplibre-gl": "5.1.0",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-markdown": "9.0.3",
		"react-router": "7.1.5",
		"zustand": "5.0.3"
	},
	"devDependencies": {
		"@babel/core": "7.29.0",
		"@babel/plugin-transform-unicode-property-regex": "7.28.6",
		"@playwright/test": "1.58.2",
		"@react-router/dev": "7.13.0",
		"@technologiestiftung/eslint-config": "0.4.1",
		"@technologiestiftung/prettier-config": "1.0.0",
		"@turf/turf": "7.3.4",
		"@types/geojson": "7946.0.16",
		"@types/node": "22.19.11",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"@typescript-eslint/eslint-plugin": "8.55.0",
		"@typescript-eslint/parser": "8.55.0",
		"@vitejs/plugin-legacy": "6.1.1",
		"@vitejs/plugin-react": "4.7.0",
		"autoprefixer": "10.4.24",
		"eslint": "9.39.2",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import": "2.32.0",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.5.0",
		"geojson": "0.5.0",
		"globals": "15.15.0",
		"postcss": "8.5.6",
		"prettier": "3.8.1",
		"serve": "14.2.5",
		"slugify": "1.6.6",
		"tailwindcss": "3.4.19",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"vite": "6.1.0",
		"vite-plugin-babel": "1.5.1",
		"vite-tsconfig-paths": "5.1.4"
	}
}
