{
	"name": "berlinpass",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "npm run build:content && vite",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint \"src/**/*\"",
		"prettier": "prettier --write .",
		"prettier:ci": "prettier --check .",
		"build:translations": "npx tsx generate-translations.ts",
		"build:content": "npx tsx generate-content-and-routes.ts && npx prettier --config prettier.config.js --write src/content/detail-pages-content.ts src/routes/detail-pages-routes.ts",
		"build:data": "tsc --module es6 --skipLibCheck --outDir dist/assets src/routes/detail-pages-routes.ts src/content/detail-pages-content.ts && tsc --module es6 --skipLibCheck --outDir dist/assets src/routes/routes.ts src/content/content.ts",
		"build:csr": "tsc -b && vite build",
		"build:ssr": "vite build --ssr src/entry-server.tsx --outDir dist/server",
		"build": "npm run build:content && npm run build:csr && npm run build:data && npm run build:ssr && npx tsx generate-html.ts",
		"preview": "npx serve dist"
	},
	"dependencies": {
		"maplibre-gl": "4.7.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.27.0",
		"zustand": "4.5.5"
	},
	"devDependencies": {
		"@technologiestiftung/eslint-config": "0.3.1",
		"@technologiestiftung/prettier-config": "1.0.0",
		"@types/geojson": "7946.0.14",
		"@types/node": "22.7.6",
		"@types/react": "18.3.11",
		"@types/react-dom": "18.3.1",
		"@typescript-eslint/eslint-plugin": "8.10.0",
		"@typescript-eslint/parser": "8.10.0",
		"@vitejs/plugin-react": "4.3.2",
		"autoprefixer": "10.4.20",
		"eslint": "9.12.0",
		"eslint-plugin-react": "7.37.0",
		"eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
		"eslint-plugin-react-refresh": "0.4.12",
		"globals": "15.11.0",
		"postcss": "8.4.47",
		"prettier": "3.3.3",
		"slugify": "1.6.6",
		"tailwindcss": "3.4.14",
		"typescript": "5.6.3",
		"vite": "5.4.9"
	}
}
